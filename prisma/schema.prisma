generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Association {
  id        String   @id @default(cuid())
  name      String   @unique
  color     String   @default("#111827")
  blurb     String   @default("")
  slug      String   @unique
  imagePath String?
  socials   Json?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
